{
    "name": "pili-io/pili-php",
    "type": "library",
    "description": "PILI is a Live Streaming Cloud Service. The PILI SDK for PHP enables PHP developers to use PILI Live Streaming Cloud Services in their PHP code for building robust applications and software.",
    "keywords": ["pili", "cloud", "live streaming", "rtmp", "hls", "m3u8"],
    "homepage": "https://github.com/pili-io",
    "license": "MIT",
    "authors": [
        {
            "name": "why404",
            "email": "why404@gmail.com",
            "homepage": "https://github.com/why404"
        }
    ],
    "support": {
        "issues": "https://github.com/pili-io/pili-php/issues"
    },
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "~5.1"
    },
    "require-dev": {
        "ext-curl": "*",
        "psr/log": "~1.0",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Pili\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pili\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
